#!/usr/bin/perl -w

# ================================================================
# John Kerl
# http://johnkerl.org
# Reverses the order of input lines.
# ================================================================

@lines = <>;
$n = scalar @lines;
for ($i = $n-1; $i >= 0; $i--) {
	$line = $lines[$i];
	chomp $line;
	print "$line\n";
}
