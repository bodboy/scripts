from math import *
n=8
for k in range(0, n+1):
	theta = 2.0 * pi * k / (1.0 * n)

	r11 =  cos(theta)
	r12 = -sin(theta)
	r21 =  sin(theta)
	r22 =  cos(theta)

	q11 = -cos(theta)
	q12 = -sin(theta)
	q21 = -sin(theta)
	q22 =  cos(theta)

	#print " %7.4f %7.4f %7.4f %7.4f %7.4f %7.4f %7.4f %7.4f" % \
	#	(r11, r12, r21, r22, q11, q12, q21, q22)

	print " %7.4f %7.4f   %7.4f %7.4f" % (r11, r12, q11, q12)
	print " %7.4f %7.4f   %7.4f %7.4f" % (r21, r22, q21, q22)
	print

