#!/usr/bin/python -Wall

import sys
from sackmat_m import *

argc = len(sys.argv)
if (argc < 2):
	print >> sys.stderr, "Usage: %s {file name 1} {file name 2}" % (sys.argv[0])
	sys.exit(1)

A = read_matrix(float, sys.argv[1])
argi = 2
while (argi < argc):
	B = read_matrix(float, sys.argv[argi])
	A = A + B
	argi += 1
print_matrix(A)
