from __future__ import division # 1/2 = 0.5, not 0.
import sys
from sackmat_m import *

argc = len(sys.argv)
if (argc < 2):
	print "Usage: %s {file names ...}" % (sys.argv[0])
	sys.exit(1)

argi = 1
while (argi < argc):
	A = read_matrix(float, sys.argv[argi])
	d = A.det()
	print d
	argi += 1
